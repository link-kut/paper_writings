@article{CAL:02,
	Author = {Callaway, Ed and Gorday, Paul and Hester, Lance and Gutierrez, Jose A. and Naeve, Marco and Heile, Bob and Bahl, Venkat},
	Journal = {{IEEE} Communications Magazine},
	Month = AUG,
	Overview = {Tutorial sobre el draft del estandar IEEE 802.15.4. El objetivo es definir un protocolo de red de bajo coste, de bajo consumo, para aplicaciones que requieren bajas tasas de transmision y que su modo de operacion sea adhoc. Comenta que 802.11 no tiene en cuenta las restricciones de consumo, y que Bluetooth al ser pensado para sustituir cables tampoco (los mecanismos posteriores que tuvieron que implementar para que fuese un protocolo para la comunicacion entre iguales implican un gran consumo de baterias. Habla de la iniciativa ZigBee el que sera el equivalente a WiFi. Tiene dos topologias en estrella (que es la habitual) y peer-to-peer. El nivel MAC emplea CSMA/CA. Se contemplan tres niveles de seguridad: no-security, access control lists y symmetric key security con AES-128. El nivel PHY opera en las bandas (895(EU)/915(USA) MHz y 2,4 GHz) emplea DSSS},
	Pages = {70--77},
	Pdf = {documentos/redes/adhoc/CAL_02.pdf},
	Title = {Home {N}etworking with {IEEE} 802.15.4: {A} {D}eveloping {S}tandard for {L}ow-{R}ate {W}ireless {P}ersonal {A}rea {N}etworks},
